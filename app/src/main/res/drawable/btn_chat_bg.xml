<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="false">
        <shape>
            <gradient android:startColor="@color/color_FF837C" android:endColor="@color/color_FF3E71" android:angle="0"/>
            <corners android:radius="14dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="@color/color_DD726C" android:endColor="@color/color_CD335C" android:angle="0"/>
            <corners android:radius="14dp"/>
        </shape>
    </item>
</selector>
